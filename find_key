public class local {

    public static boolean findKey(int arr[], int key) {
        for (int k : arr) {
            if (k == key) {
                return true;
            }
        }
      return false; 
    }

    public static void main(String[] args) {
        int arr[] = {6, 3, 9, 8, 2, 5, -5};
        int key = 10;
       System.out.print(findKey(arr,key)); 
        
    }
}
