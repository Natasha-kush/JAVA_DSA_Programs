import java.lang.Math;

public class area_of_circle {

    public static float areafind(float r) {
        return (float) (Math.PI * r * r);

    }

    public static void main(String[] args) {
        float r = 5;
        float area = areafind(r);
        System.out.printf("%.5f%n",area);

    }
}
