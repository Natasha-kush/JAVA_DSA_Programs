package datastructure;
public class Fibonacci {
   //0,1,2,........The two first Fibonacci numbers are 0 and 1, and the 
   //next Fibonacci number is always the sum of the two previous numbers, so we get 0, 1, 1, 2, 3, 5, 8, 13, 21, ...
    public static void main(String[]args){
    int a=0;
    int b=1;
    int c;
    for(int i=0; i<10; i++){
    c=a+b;
    System.out.println(c);
   a=b;
   b=c;
   
    }
    }
}
