public class local {

   public static int findKey(int arr[], int count, int key) {
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == key) {
                count++;
            }
        }
        return count;
    }

    public static void main(String[] args) {
        int arr[] = {6, 3, 9, 8, 2, 2, 2, 5, -5};
        int key = 2;
        int count = 0;
        System.out.println(findKey(arr, count, key));
    }
}
