public class compute_x_tothe_power_n {

    public static int power(int x, int n) {
        //base case
        if (n == 0) {
            return 1;
        }
        int power1 = power(x, n - 1);
        int calculate = power1 * x;
        return calculate;

    }

    public static void main(String[] args) {
        System.out.print(power(2, 5));

    }
}
