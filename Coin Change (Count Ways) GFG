class Solution {
    public int count(int coins[], int sum) {
        // code here.
         int[] dp = new int[sum + 1];
        dp[0] = 1; // sum 0 banane ka ek hi tareeka: koi coin na lena

        for (int coin : coins) {
            for (int i = coin; i <= sum; i++) {
                dp[i] += dp[i - coin];
            }
        }

        return dp[sum];
    }
}
